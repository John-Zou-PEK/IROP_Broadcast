    "import numpy as np\n",
    "\n",
    "\n",
    "#Generates N nodes and returns a list of tuples of their position.\n",
    "def N_node_gen(N):\n",
    "    node_list = list()\n",
    "    for _ in range(N):\n",
    "        node_list.append((np.random.random(), np.random.random()))\n",
    "    return node_list\n",
    "\n",
    "N_node_gen(5)"
